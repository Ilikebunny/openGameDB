<!DOCTYPE html>
<html>
    {{ include('layout/head.html.twig') }}
    <body>
        {{ include('layout/header.html.twig') }}
        <div id="main" class="container">

            <section class="panel row">
                    {{ wo_render_breadcrumbs() }}
              {#  <header class="panel-heading col-md-12">
                    <h2>{% block panel_head %}{% endblock %}</h2>
                </header>#}
                {% block menu_content %}
                {% endblock %}
                <div class="panel-body col-md-12">
                    {{ include('layout/flash_message_block.html.twig') }}
                    {% block body %}
                    {% endblock %}
                    {% if pagerHtml is defined %}
                        {{ pagerHtml | raw}}
                    {% endif %}
                </div>
            </section>

        </div>
        <div id="footer" class="container-fluid">
            {{ include('layout/footer.html.twig') }}
        </div>
        {{ include('layout/js.html.twig') }}
    </body>
</html>